<editor>
  <formElement>
    <label>Agent Name</label>
    <property>agent</property>
    <documentation></documentation>
    <type>entry</type>
    <value></value>
    <required>1</required>
  </formElement>

  <formElement>
    <label>initMemory</label>
    <property>initMemory</property>
    <documentation>a value in mb or a %</documentation>
